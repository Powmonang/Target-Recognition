<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzk.mapper.WarnTypeMapper">

    <select id="findWarnTypes" parameterType="com.lzk.entity.WarnType" resultType="com.lzk.entity.WarnType">
        SELECT * FROM warnType
        <where>
            <if test="id!=null">
                id = #{id}
            </if>
        </where>
    </select>
</mapper>